{
	"[lua]": {
		"editor.quickSuggestions": {
			"comments": "inline",
			"other": "on",
			"strings": "on"
		},
		"editor.semanticHighlighting.enabled": true,
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "JohnnyMorganz.stylua"
	},
	"Lua.runtime.special": {
		"include": "require"
	},
	"Lua.diagnostics.disable": [
		"undefined-global"
	]
}